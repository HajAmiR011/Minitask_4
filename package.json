{
  "name": "ts-tailwind",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typescript": "npx tsc --watch ./public/ts/main.ts --outdir ./public/js",
